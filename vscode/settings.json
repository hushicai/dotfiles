{
    "editor.fontSize": 14,
    "workbench.colorTheme": "Solarized Dark",
    "breadcrumbs.enabled": true,
    "vim.leader": ",",
    "vim.foldfix": true,
    "vim.useCtrlKeys": true,
    "vim.handleKeys": {
        "<c-o>": false,
        "<c-i>": false,
        "<c-a>": false,
        "<c-e>": false
    },
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<esc>"]
        }
    ],
    "vim.easymotion": true
}
